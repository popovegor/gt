    <UseSandboxSettings>True</UseSandboxSettings>
    <SandboxTargetConnectionString>Data Source=.\mssqlserver2008;Integrated Security=True;Pooling=False</SandboxTargetConnectionString>
    <SandboxDeploymentConfigFile>Properties\Developmnet.sqldeployment</SandboxDeploymentConfigFile>
    <SandboxSqlCommandVariablesFile>Properties\Development.sqlcmdvars</SandboxSqlCommandVariablesFile>
    <SandboxTargetDatabase>GT</SandboxTargetDatabase>
  </PropertyGroup>
</Project>